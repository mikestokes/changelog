{
  "name": "productlog",
  "description": "Awesome changelog, release notes and what's new widget for your product. Run on our cloud, or host you own https://productlog.dev",
  "version": "0.1.0",
  "engines": {
    "node": "12.x"
  },
  "scripts": {
    "dev": "sapper dev",
    "watch:tailwind": "postcss static/tailwind.css -o static/index.css -w",
    "build:tailwind": "NODE_ENV=production postcss static/tailwind.css -o static/index.css",
    "build": "npm run build:tailwind && sapper build --legacy",
    "export": "sapper export --legacy",
    "start": "node __sapper__/build",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "test": "run-p --race dev cy:run"
  },
  "dependencies": {
    "@polka/send-type": "^0.5.2",
    "compression": "^1.7.1",
    "dayjs": "^1.8.18",
    "firebase-admin": "^8.9.0",
    "marked": "^4.0.10",
    "polka": "next",
    "sirv": "^0.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.7.7",
    "@babel/runtime": "^7.7.7",
    "@fullhuman/postcss-purgecss": "^1.3.0",
    "@rollup/plugin-replace": "^2.3.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^7.0.25",
    "postcss-cli": "^6.1.3",
    "postcss-load-config": "^2.1.0",
    "rollup": "^1.27.14",
    "rollup-plugin-babel": "^4.0.2",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-svelte": "^5.0.1",
    "rollup-plugin-terser": "^4.0.4",
    "sapper": "^0.27.0",
    "svelte": "^3.16.7",
    "svelte-preprocess": "^3.3.0",
    "tailwindcss": "^1.1.4"
  }
}
